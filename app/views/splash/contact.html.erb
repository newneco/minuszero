<div class="container">
	<h1 class="page-title text-center">Contact Us</h1>
	<div class="contact-form col-xs-12 col-md-6 col-md-offset-3">
		<form id="contactForm">
		  <div class="form-group">
		    <label for="contactEmail">Email address</label>
		    <input type="email" class="form-control" id="contactEmail" placeholder="Email">
		  </div>
		  <div class="form-group">
		  	<label for="contactSubject">Subject</label>
		    <select class="form-control" id="contactSubject">
		    	<option>Select Subject</option>
			  	<option>Inquiries regarding purchased tickets and wristbands</option>
			  	<option>General information and questions about Minus Zero</option>
			  	<option>Sponsorship Inquiries</option>
			  	<option>Press/Media Pass Inquiries</option>
			</select>
		  </div>
		  <div class="form-group">
		  	<label for="contactMessage">Body</label>
		    <textarea class="form-control" rows="3" id="contactMessage"></textarea>
		  </div>
          <div class="form-group">
            <label for="captcha" id="captcha_label"></label>
            <input type="captcha" class="form-control" id="captchaObject" placeholder="Please prove you aren't a robot.">
          </div>
		  <button id="bt_contact" type="submit" class="btn btn-default">Submit</button>
		</form>	
	</div>
</div>


